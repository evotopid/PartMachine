<%= form_for @source, remote: true do |f| %>
  <%= save_errors @source %>
  
  <%= f.label :name %>
  <%= f.text_field :name %>
  
  <%= f.label :url %>
  <%= f.text_field :url %>
  
  <%= f.label :part_id %>
  <%= f.select :part_id, @parts, selected: @part_id, include_blank: true %>
  
  <%= f.submit %>
  <input type="button" value="Cancel" onclick="history.back();">
<% end %>
